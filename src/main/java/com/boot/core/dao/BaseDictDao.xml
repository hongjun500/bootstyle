<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boot.core.dao.BaseDictDao">

    <!-- 根据类别代码查询 -->
    <select id="selectBaseDictByTypeCode" parameterType="String" resultType="baseDict">
        select *
        from base_dict
        where dict_type_code = #{typecode}
    </select>
    <select id="query" parameterType="String" resultType="baseDict">
        select * from base_dict where dict_id=${dfd}
    </select>
</mapper>